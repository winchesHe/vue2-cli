{
  "name": "@winches/vue2-cli",
  "type": "module",
  "version": "0.0.1",
  "description": "",
  "author": {
    "name": "winches"
  },
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "bin": {
    "vue2-cli": "dist/cli.js"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "tsup --watch",
    "build": "tsup-node",
    "start": "tsx src/cli.ts",
    "lint": "eslint --cache --ext ts,tsx,vue .",
    "changlog": "npx conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "bumpp --execute=\"npm run changlog\" --all"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "require": "./dist/cli/index.js",
      "import": "./dist/cli/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@inquirer/input": "^1.2.7",
    "@inquirer/select": "^1.2.7",
    "@vuese/parser": "^2.10.3",
    "@winches/utils": "^0.0.8",
    "chalk": "^5.2.0",
    "find-up": "^6.3.0",
    "log-update": "^5.0.1",
    "prettier": "^3.0.1",
    "read-pkg": "^8.0.0",
    "sisteransi": "^1.0.5",
    "yargs": "^17.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.0",
    "@types/node": "^18.7.18",
    "@types/yargs": "^17.0.24",
    "eslint": "^8.46.0",
    "lint-staged": "^13.1.2",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix"
    ]
  }
}
